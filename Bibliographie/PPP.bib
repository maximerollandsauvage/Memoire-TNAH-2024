@article{pallezRecherchePartenarialeEstelle2014,
  title = {La Recherche Partenariale Est-Elle Soluble Dans Les Politiques Publiques de Recherche ?:},
  shorttitle = {La Recherche Partenariale Est-Elle Soluble Dans Les Politiques Publiques de Recherche ?},
  author = {Pallez, Fr{\'e}d{\'e}rique},
  year = {2014},
  month = feb,
  journal = {Annales des Mines - R{\'e}alit{\'e}s industrielles},
  volume = {F{\'e}vrier 2014},
  number = {1},
  pages = {10--15},
  issn = {1148-7941},
  doi = {10.3917/rindu.141.0010},
  urldate = {2024-08-28},
  abstract = {En France, la recherche partenariale est consid{\'e}r{\'e}e comme un des moyens d'augmenter la comp{\'e}titivit{\'e} des entreprises. {\`A} ce titre, elle est soutenue par divers dispositifs publics. Ses principes semblent en outre compatibles avec les principaux axes des politiques publiques de recherche, qui mettent de plus en plus l'accent sur le n{\'e}cessaire partenariat entre recherche et industrie. Cela dit, quand on s'int{\'e}resse aux moteurs principaux du comportement des acteurs de la recherche publique, {\`a} savoir les chercheurs ou les enseignants-chercheurs, on constate que des m{\'e}canismes puissants (tenant notamment aux modes d'{\'e}valuation et aux modes de financement) freinent encore ces rapprochements. La voie permettant de surmonter ces obstacles est {\'e}troite.},
  keywords = {PPP}
}

